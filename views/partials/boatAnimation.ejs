<div id="boatContainer" style="position: relative;"></div>
<script src="https://cdnjs.cloudflare.com/ajax/libs/bodymovin/5.7.1/lottie.min.js"></script>
<script>
    // ID des Container-Elements
    const container = document.getElementById('boatContainer');
  
    // Pfad zur Lottie JSON-Datei
    const animationData = 'animations/summer-vacation.json';
  
    // Lade und zeige die Animation
    const anim = lottie.loadAnimation({
      container: container,
      renderer: 'svg', // WÃ¤hle den Renderer (z. B. 'svg' oder 'canvas')
      loop: true,
      autoplay: true,
      path: animationData // Pfad zur Lottie JSON-Datei
    });
    console.log("yes");
  </script>